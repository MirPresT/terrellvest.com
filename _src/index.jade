- var aboutClass = "active"
- var blogClass = "inactive"
- var portfolioClass = "inactive"

doctype HTML
html(lang="en")
  head
    meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(name="apple-mobile-web-app-capable" content="yes")
    meta(name="mobile-web-app-capable" content="yes")
    link(href="assets/styles/main.css" rel="stylesheet" type="text/css")
  body
    header
      nav
        .row
          include ./assets/markup/nav.jade
    main#about-content
      article.row
        header.sub-header.col.col--2-of-8
          h1.page-title Introduction
        div.md-content.col.col--6-of-8
          img(id="profile-pic" src="assets/images/Profile.jpg" alt="Profile image of Terrell Vest")
          != postContent
  
    
    
