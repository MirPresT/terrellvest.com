- var aboutClass = "active"
- var blogClass = "inactive"
- var portfolioClass = "inactive"

doctype HTML
html(lang="en")
  head
    meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(name="apple-mobile-web-app-capable" content="yes")
    meta(name="mobile-web-app-capable" content="yes")
    link(href="assets/styles/main.css" rel="stylesheet" type="text/css")
  body
    .page-container
      header#site-header
        include ./assets/markup/nav.jade
      
      main#about-content
        article#about-composition
          header.sub-header
            h1.page-title Introduction
          #about-md-content.md-content
            img(id="profile-pic" src="assets/images/Profile.jpg" alt="Profile image of Terrell Vest")
            != postContent
    
      
    
