- var aboutClass = "active";
- var blogClass = "inactive";
- var portfolioClass = "inactive";
- var contactClass = "inactive";

- var logoURL = "assets/images/sprites/sprites.svg#logo";

doctype HTML
html(lang="en")
  head
    meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(name="apple-mobile-web-app-capable" content="yes")
    meta(name="mobile-web-app-capable" content="yes")
    link(href="assets/styles/main.css" rel="stylesheet" type="text/css")
  body
    div.nav_container
    header
      nav
        div.row
          include ./assets/markup/logo.jade
          include ./assets/markup/nav.jade
    main#about-content
      article.row
        header.sub-header.col
          h1.page-title Introduction
        div.md-content.col
          img(id="profile-pic" src="assets/images/Profile.jpg" alt="Profile image of Terrell Vest")
          != postContent
    script(src="assets/scripts/g_a_code.js")
    
    
