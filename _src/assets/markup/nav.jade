nav.row.nav-parent-row
  box.wrapper
    .row.center--col-x.nav-wrapper-row
      .box.auto--width.logo-box
        a(href="/")
          svg#nav-logo-svg
            use(xlink:href=logoURL)
      .box.auto--width.nav-list-box
        .row.nav-list.center--row-y
          a.box.nav-list-item-box.auto--width(href="/" id="about-link" class=aboutClass): li About
          a.box.nav-list-item-box.auto--width(href="/portfolio" id="portfolio-link" class=portfolioClass): li Portfolio
          a.box.nav-list-item-box.auto--width(href="/blog" id="blog-link" class=blogClass): li Blog
          a.box.nav-list-item-box.auto--width(href="/contact" id="contact-link" class=contactClass): li Contact

    
    
