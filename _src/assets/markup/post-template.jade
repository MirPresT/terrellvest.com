- var aboutClass = "inactive";
- var blogClass = "active";
- var portfolioClass = "inactive";
- var contactClass = "inactive";

- var logoURL = "../../assets/images/sprites/sprites.svg#logo"

doctype HTML
html(lang="en")
  head
    meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(name="apple-mobile-web-app-capable" content="yes")
    meta(name="mobile-web-app-capable" content="yes")
    link(href="../../assets/styles/main.css" rel="stylesheet" type="text/css")
  body
  .page-wrapper
    include ../assets/markup/nav.jade
      test test test
    main.blog-post-content
      article.row
        div#blog-header-wrap.col
          header
            h1.page-title #{postTitle}
          div.blog-post-description
            span #{postDate}
        div#blog-post-md-content.col
          != postContent
        #back-blog-container
          a#back-blog.col(href="/blog") All Posts
      #disqus_thread
    script(src="../../assets/scripts/g_a_code.js")
    script(src="../../assets/scripts/disqus.js")
